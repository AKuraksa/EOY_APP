<UserControl x:Class="EOY_APP.Forms.Admin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EOY_APP.Forms"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Loaded="UserControl_Loaded">
    <Grid HorizontalAlignment="Left" Width="1524">
        <TextBlock x:Name="asad" HorizontalAlignment="Left" Height="54" Margin="24,104,0,0" TextWrapping="Wrap" Text="Uživatel: " VerticalAlignment="Top" Width="136" FontSize="36" Foreground="#FF020202"/>
        <TextBlock HorizontalAlignment="Left" Height="122" Margin="24,21,0,0" TextWrapping="Wrap" Text="ID:" VerticalAlignment="Top" Width="60" FontSize="36" Foreground="#FF020202" RenderTransformOrigin="1.255,0.783"/>
        <TextBlock HorizontalAlignment="Left" Height="51" Margin="24,178,0,0" TextWrapping="Wrap" Text="E-mail:" VerticalAlignment="Top" Width="120" FontSize="36" Foreground="#FF020202"/>
        <TextBlock x:Name="idUser" HorizontalAlignment="Left" Height="61" Margin="84,21,0,0" TextWrapping="Wrap" Text="id" VerticalAlignment="Top" Width="849" FontSize="36" Foreground="#FF020202" RenderTransformOrigin="1.255,0.783"/>
        <TextBlock x:Name="nameUser" HorizontalAlignment="Left" Height="61" Margin="183,104,0,0" TextWrapping="Wrap" Text="name" VerticalAlignment="Top" Width="1315" FontSize="36" Foreground="#FF020202" RenderTransformOrigin="1.255,0.783"/>
        <TextBlock x:Name="mailUser" HorizontalAlignment="Left" Height="61" Margin="144,178,0,0" TextWrapping="Wrap" Text="mail" VerticalAlignment="Top" Width="1354" FontSize="36" Foreground="#FF020202" RenderTransformOrigin="1.255,0.783"/>

    </Grid>
</UserControl>
