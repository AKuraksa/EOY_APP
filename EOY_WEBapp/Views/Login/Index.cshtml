@model EOY_WEBapp.Dto.LoginDto;


@{
    ViewData["Title"] = "Přihlášení";
}

<link rel="stylesheet" href="~/css/styleIndex.css" />

<head class="topPanel">
    <title></title>
   
</head>
 <div>
     <img alt="logo" class="logo" src="~/Pictures/eoyLogo.png"/>
 </div>

   <body>
    <form method="get" action="/LoginController/LogIn">
  
    <div class="centered-div">
        <div class="panelLogin">
            <div class="textbox_container">
                <ul>
                    <li class="no-bullet">
                            <input asp-for="Username" type="text" placeholder="uživatelské jméno">
                    </li>
                </ul>
                <ul>
                        <li class="no-bullet"><input asp-for="Password" type="password" placeholder="heslo"></li>
                </ul>
                <ul>
                        <li class="no-bullet"><button class="btnTemplate" id="loginButton">Přihlásit se</button></li>
                </ul>
            </div>
        </div>
    </div>
    </form>
</body>


